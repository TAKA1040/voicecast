{
  "name": "voicecast",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull", 
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --linked > lib/supabase.types.ts",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "setup": "bash scripts/setup.sh",
    "logs": "vercel logs --follow"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.856.0",
    "@aws-sdk/s3-request-presigner": "^3.856.0",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.43.5",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.19",
    "cross-env": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
