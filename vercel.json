{
  "version": 2,
  "redirects": [
    {
      "source": "/auth/callback",
      "destination": "/admin",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}